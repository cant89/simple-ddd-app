{
  "name": "stupid-app",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Davide Cantelli <cant1989@gmail.com>",
  "license": "ISC",
  "homepage": "",
  "scripts": {
    "tslint:check": "tslint 'src/**/*.ts?(x)' && tsc",
    "start": "webpack-dev-server --config=webpack/webpack.local.config.js --color --progress",
    "build:prod": "webpack --config webpack/webpack.prod.config.js --progress --colors",
    "build:beta": "webpack --config webpack/webpack.beta.config.js --progress --colors",
    "build:stage": "webpack --config webpack/webpack.stage.config.js --progress --colors",
    "test": "jest --verbose --watchAll",
    "test:once": "jest --verbose",
    "test:once:coverage": "jest -i --verbose --coverage",
    "check-types": "tsc"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.1",
    "axios": "^0.21.1",
    "connected-react-router": "6.3.1",
    "dot-prop-immutable": "^1.5.0",
    "fast-deep-equal": "^2.0.1",
    "history": "^4.9.0",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "query-string": "^6.8.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.2",
    "redux-saga-routines": "^3.1.3",
    "redux-saga-test-plan": "^4.0.0-beta.3",
    "reselect": "^4.0.0",
    "ts-debounce": "^1.0.0",
    "typesafe-actions": "^4.4.0",
    "url-loader": "^1.1.2",
    "utility-types": "^3.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/dot-prop-immutable": "^1.5.0",
    "@types/gapi": "0.0.36",
    "@types/gapi.auth2": "0.0.50",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/react-router": "^5.0.0",
    "@types/react-router-config": "^1.1.3",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-router-redux": "^5.0.18",
    "@types/react-virtualized": "^9.21.2",
    "@types/recompose": "^0.30.6",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-saga-routines": "^3.1.0",
    "@types/reselect": "^2.2.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^2.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "imagemin-lint-staged": "^0.4.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "redux-mock-store": "^1.5.4",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-s3-plugin": "^1.0.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm run tslint:check && npm run test:once:coverage"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^.+\\.(png|jpg|gif|svg)$": "<rootDir>/helpers/imageMock.js",
      "^#/(.*)": "<rootDir>/src/$1"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "notify": true,
    "resetMocks": true,
    "coveragePathIgnorePatterns": [
      "__tests__",
      "coverage/",
      "src/static",
      "webpack/"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**"
    ],
    "globals": {
      "__TEST__": true
    },
    "setupFilesAfterEnv": [
      "<rootDir>jest/config.js"
    ]
  }
}
